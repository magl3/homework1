@model IEnumerable<SBCHomework.CoreModels.User>

    <form asp-controller="Search" asp-action="Index" method="get">
        <p>
            UniqKey: <input type="text" name="SearchStr" />
            <input type="submit" value="Filter" />
        </p>
    </form>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UniqKey)
                </th>
                <th>

                </th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UniqKey)
                </td>
                <td>
                    <a class="buttonns" asp-controller="Chat" asp-action="Index" asp-route-id="@item.Id">Chat</a>
                </td>
            </tr>
        }
    </table>